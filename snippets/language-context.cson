# "Name, short description":
#   prefix: "what you type"
#   body: "what you get"
#
".text.tex.context":
  "\\about":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/about"
    description: "Provides the complete title of a reference"
    prefix: "about"
    body: "\\about" +
      "{${1:text}}" +
      "[${2:reference}]" +
      "$3"
  "\\adaptlayout":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/adaptlayout"
    description: "Adapt the layout of one page to fit more or less lines in it"
    prefix: "adaptlayout"
    body: "\\adaptlayout" +
      "[${2:page number(s)}]" +
      "[${3:lines=-1,height=1em}]\n" +
      "$4"
  "\\arg":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/arg"
    description: "Surround given text with braces"
    prefix: "arg"
    body: "\\arg" +
      "{${1:name}}" +
      "$2"
  "\\at":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/at"
    description: "Provides the page number of a reference"
    prefix: "at"
    body: "\\at" +
      "{${1:prefix}}" +
      "{${2:suffix}}" +
      "[${3:reference}]" +
      "$4"
  "\\atpage":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/atpage"
    description: "Reference with page number or 'see above' if on same page"
    prefix: "atpage"
    body: "\\atpage" +
      "[${1:reference}]" +
      "$2"
  "\\blank":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/blank"
    description: "Insert vertical blank space"
    prefix: "blank"
    body: "\\blank" +
      "[${1:big}]" +
      "$2"
  "\\cap":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/cap"
    description: "Small caps text inside"
    prefix: "cap"
    body: "\\cap" +
      "{${1:name}}" +
      "$2"
  "\\chapter":
    description: "Start a chapter"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/chapter"
    prefix: "chapter"
    body: "\\chapter" +
      "[${1:reference}]" +
      "{${2:title}}" +
      "$3"
  "\\startchapter":
    description: "Start a chapter"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/startchapter"
    prefix: "startchapter"
    body: "\\startchapter" +
      "[title={${1:title}},reference=${2:reference}]\n" +
      "$3\n" +
      "\\\\stopchapter\n"
  "\\startsection":
    description: "Start a section"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/startsection"
    prefix: "startsection"
    body: "\\startsection" +
      "[title={${1:title}},reference=${2:reference}]\n" +
      "$3\n" +
      "\\\\stopsection\n"
  "\\startsubsection":
    description: "Start a subsection"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/startsubsection"
    prefix: "startsubsection"
    body: "\\startsubsection" +
      "[title={${1:title}},reference=${2:reference}]\n" +
      "$3\n" +
      "\\\\stopsubsection\n"
  "\\clip":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/clip"
    description: '''
      Crop image to width x height, starting at hoffset x voffset
      OR use piece at position (x, y) if number of pieces is
      specified by \\setupclipping.
    '''
    prefix: "clip"
    body: "\\clip" +
      "[x=${1:number}, y=${2:number}, hoffset=${3:dim}, voffset=${4:dim}, width=${5:dim}, height=${6:dim}]" +
      "{${7:figure or text}}" +
      "$8"
  "\\color":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/color"
    description: "Change text color"
    prefix: "color"
    body: "\\color" +
      "[${1:color}]" +
      "{${2:text to colorize}}" +
      "$3"
  "\\column":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/column"
    description: "Insert column break"
    prefix: "column"
    body: "\\column\n" +
      "$0"
  "\\convertnumber":
    description: "Convert a number into a different form, e.g. to Roman numerals, bullets, etc."
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/convertnumber"
    prefix: "convertnumber"
    body: "\\convertnumber" +
      "{${1:mode}}" +
      "{${2:number}}" +
      "$3"
  "\\crlf":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/crlf"
    description: "Carriage return (new line)"
    prefix: "crlf"
    body: "\\crlf\n" +
      "$1"
  "\\currentdate":
    description: "Insert the current date"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/currentdate"
    prefix: "currentdate"
    body: "\\currentdate" +
      "[${1:weekday,month,day,year}]" +
      "$2"
  "\\date":
    description: "Insert a date"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/date"
    prefix: "date"
    body: "\\date" +
      "[d=${1:name},m=${2:name},y=${3:name}}]" +
      "[${4:weekday,month,day,year}]" +
      "$5"
  "\\define":
    description: "Define a new macro (like \\def)"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/define"
    prefix: "define"
    body: "\\define" +
      "${1:[${2:number of arguments}]}" +
      "\\\\${3:command}" +
      "{${4:contents}}\n" +
      "$5"
  "\\defineblank":
    description: "Define a new vertical space"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineblank"
    prefix: "defineblank"
    body: "\\defineblank" +
      "[${1:measure name}]" +
      "[${2:dimension}]\n" +
      "$3"
  "\\defineblock":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineblock"
    description: "Define a reusable text format (e.g. questions)"
    prefix: "defineblock"
    body: "\\defineblock" +
      "[${1:name of the block}]\n" +
      "$2"
  "\\definebodyfont":
    description: "Define a new body font combination"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definebodyfont"
    prefix: "definebodyfont"
    body: "\\definebodyfont" +
      "[${1:5pt name 12pt default}]" +
      "[${2:rm ss tt mm hw cg}]" +
      "[${3:options}]\n" +
      "$4"
  "\\definebodyfontenvironment":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definebodyfontenvironment"
    description: '''Set up font scaling for "[tf]a-[tf]"d'''
    prefix: "definebodyfontenvironment"
    body: "\\definebodyfontenvironment" +
      "[${1:font size}]" +
      "[${a=2pt, d=4pt}]\n" +
      "$3"
  "\\definecolor":
    description: "Define a new named color"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definecolor"
    prefix: "definecolor"
    body: "\\definecolor" +
      "[${1:color name}]" +
      "[${2:c=0,m=0,y=0,k=0,r=0,g=0,b=0,h=FF0000}]\n" +
      "$3"
  "\\definecolorgroup":
    description: "Define a new set of colors"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definecolorgroup"
    prefix: "definecolorgroup"
    body: "\\definecolorgroup" +
      "[${1:color set name}]" +
      "${2:[${3:rgb cmyk gray s}]}" +
      "[${1.00:0.85:0.80, name}]" +
      "$5"
  "\\definecombinedlist":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definecombinedlist"
    description: "Merge lists created by \\definelist e.g. into a ToC"
    prefix: "definecombinedlist"
    body: "\\definecombinedlist" +
      "[${1:name}]" +
      "[${2:chapter,section}]" +
      "[${3:criterium=all}]\n" +
      "$4"
  "\\defineconversion":
    description: "Define a new number-to-symbol conversion"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineconversion"
    prefix: "defineconversion"
    body: "\\defineconversion" +
      "[${1:conversion name}]" +
      "[${2:list of strings to convert numbers into}]\n" +
      "$3"
  "\\definedescription":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definedescription"
    description: "Define a new description with an associated style"
    prefix: "definedescription"
    body: "\\definedescription" +
      "[${1:name}]" +
      "${2:[${3: \\\\setupdescription parameters}]}" +
      "$4"
  "\\defineenumeration":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineenumeration"
    description: "Define the commands for a numbered text element"
    prefix: "defineenumeration"
    body: "\\defineenumeration" +
      "[${1:name}]" +
      "${4:[${5:\\\\setupenumeration parameters}]}" +
      "$6"
  "\\definefiguresymbol":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definefiguresymbol"
    description: "Define a symbol using a figure"
    prefix: "definefiguresymbol"
    body: "\\definefiguresymbol" +
      "[${1:name}]" +
      "[${2:text}]" +
      "${3:[${4:\\\\externalfigure parameters}]}" +
      "$5"
  "\\definefloat":
    description: "Define a new kind of float"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definefloat"
    prefix: "definefloat"
    body: "\\definefloat" +
      "[${1:singular name}]" +
      "[${2:plural name}]" +
      "${3:[${4:parent}]}" +
      "$5"
  "\\definefont":
    description: "Define a new named font setting"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definefont"
    prefix: "definefont"
    body: "\\definefont" +
      "[${1:name}]" +
      "[${2:Sans*default at 12pt}]" +
      "$3"
  "\\definedfont":
    description: "Immediately switch to a font setting"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definedfont"
    prefix: "definedfont"
    body: "\\definedfont" +
      "[${1:name}]" +
      "$2"
  "\\defineframed":
    description: "Define a custom frame"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineframed"
    prefix: "defineframed"
    body: "\\defineframed" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setupframed parameters}]}" +
      "$4"
  "\\defineframedtext":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineframedtext"
    prefix: "defineframedtext"
    body: "\\defineframedtext" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setupframedtext parameters}]}" +
      "$4"
  "\\definehead":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definehead"
    prefix: "definehead"
    body: "\\definehead" +
      "[${1:name}]" +
      "[${2:parent head style}]" +
      "${3:[${4:see \\\\setuphead parameters}]}" +
      "$5"
  "\\defineindenting":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineindenting"
    prefix: "defineindenting"
    body: "\\defineindenting" +
      "[${1:name}]" +
      "[${2:see \\\\setupindentation parameters}]" +
      "$3"
  "\\definelabel":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definelabel"
    description: "Define an autonumbered label type, including a command to place one"
    prefix: "definelabel"
    body: "\\definelabel" +
      "[${1:name}]" +
      "[${2:text=${3:name}, location=${4:name}$5}]" +
      "$6"
  "\\definelist":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definelist"
    prefix: "definelist"
    body: "\\definelist" +
      "[${1:name}]" +
      "${4:[see \\\\setuplist parameters]}" +
      "$6"
  "\\definemakeup":
    description: "Define a new makeup (special page layout)"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definemakeup"
    prefix: "definemakeup"
    body: "\\definemakeup" +
      "[${1:name}]" +
      "[${2:see \\\\setuplayout parameters}]" +
      "$3"
  "\\definemarking":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definemarking"
    prefix: "definemarking"
    body: "\\definemarking" +
      "[${1:name}]" +
      "${2:[${3:name}]}" +
      "$4"
  "\\defineoverlay":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineoverlay"
    prefix: "defineoverlay"
    body: "\\defineoverlay" +
      "[${1:name}]" +
      "[${2:text}]" +
      "$3"
  "\\definepalet":
    description: "Define a color palet"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definepalet"
    prefix: "definepalet"
    body: "\\definepalet" +
      "[${1:name}]" +
      "[${2:alert=green, calm=grey, name}]" +
      "$3"
  "\\definepapersize":
    description: "Define a new paper size"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definepapersize"
    prefix: "definepapersize"
    body: "\\definepapersize" +
      "[${1:name}]" +
      "[${2:.width=${3:dim}, height=${4:dim}}]\n" +
      "$5"
  "\\defineparagraphs":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineparagraphs"
    prefix: "defineparagraphs"
    body: "\\defineparagraphs" +
      "[${1:name}]" +
      "[${2:before=${3:name}, after=${4:name}}]" +
      "$5"
  "\\definereferenceformat":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definereferenceformat"
    prefix: "definereferenceformat"
    body: "\\definereferenceformat" +
      "[${1:name}]" +
      "[${2:left=${3:name}, right=${4:name}, text=${5:name}}]" +
      "$6"
  "\\defineregister":
    description: "Define a new index"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineregister"
    prefix: "defineregister"
    body: "\\defineregister" +
      "[${1:name}]" +
      "[${2:name}]" +
      "$3"
  "\\definesection":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definesection"
    prefix: "definesection"
    body: "\\definesection" +
      "[${1:name}]" +
      "$2"
  "\\definesectionblock":
    description: "Define a new 'matter' section"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definesectionblock"
    prefix: "definesectionblock"
    body: "\\definesectionblock" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setupsectionblock parameters}]}" +
      "$4"
  "\\definesorting":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definesorting"
    prefix: "definesorting"
    body: "\\definesorting" +
      "[${1:singular}]" +
      "[${2:plural}]" +
      "${3:[${4:command}]}" +
      "$5"
  "\\definestartstop":
    description: "Define a new environment"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definestartstop"
    prefix: "definestartstop"
    body: "\\definestartstop" +
      "[${1:name}]" +
      "[${2:before=${3:name}, after=${4:name}, style=${5:name}$6}]" +
      "$7"
  "\\definesymbol":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definesymbol"
    prefix: "definesymbol"
    body: "\\definesymbol" +
      "[${1:name}]" +
      "[${2:text}]" +
      "$3"
  "\\definesynonyms":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definesynonyms"
    description: "Sort of alias for commands, listed with \\\\placelistofname"
    prefix: "definesynonyms"
    body: "\\definesynonyms" +
      "[${1:singular}]" +
      "[${2:plural}]" +
      "[${3:command}]" +
      "${4:[${5:description text}]}" +
      "$6"
  "\\definetabletemplate":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetabletemplate"
    prefix: "definetabletemplate"
    body: "\\definetabletemplate" +
      "[${1:name}]" +
      "[${2:style of table}]" +
      "$3"
  "\\definetabulate":
    description: "Define a custom tabulate setup"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetabulate"
    prefix: "definetabulate"
    body: "\\definetabulate" +
      "[${1:name}]" +
      "${2:[${3:name of variant or sub-part}]}" +
      "[${4:text}]" +
      "$5"
  "\\definetext":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetext"
    prefix: "definetext"
    body: "\\definetext" +
      "[${1:name}]" +
      "[${2:header footer}]" +
      "[${3:name}]" +
      "${4:[${5:name}]}" +
      "${6:[${7:name}]}" +
      "$8"
  "\\definetextposition":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetextposition"
    prefix: "definetextposition"
    body: "\\definetextposition" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setuppositioning parameters}]}" +
      "$4"
  "\\definetextvariable":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetextvariable"
    prefix: "definetextvariable"
    body: "\\definetextvariable" +
      "[${1:see \\\\setuptextvariable}]" +
      "${2:[${3:name}]}" +
      "${4:[${5:options}]}" +
      "$6"
  "\\definetype":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetype"
    prefix: "definetype"
    body: "\\definetype" +
      "[${1:name}]" +
      "[${2:see \\\\setuptype}]" +
      "$3"
  "\\definetyping":
    description: "Define a custom typing (source code, verbose) setup"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/definetyping"
    prefix: "definetyping"
    body: "\\definetyping" +
      "[${1:name}]" +
      "[${2:see \\\\setuptyping}]" +
      "$3"
  "\\defineversion":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/defineversion"
    prefix: "defineversion"
    body: "\\defineversion" +
      "[${1:name}]" +
      "[${2:number}]" +
      "$3"
  "\\determineheadnumber":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/determineheadnumber"
    prefix: "determineheadnumber"
    body: "\\determineheadnumber" +
      "[${1:section}]" +
      "$2"
  "\\determinelistcharacteristics":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/determinelistcharacteristics"
    prefix: "determinelistcharacteristics"
    body: "\\determinelistcharacteristics" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setuplist}]}" +
      "$4"
  "\\disableinteractionmenu":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/disableinteractionmenu"
    prefix: "disableinteractionmenu"
    body: "\\disableinteractionmenu" +
      "[${1:left right top bottom name}]" +
      "[${2:ref, ...}]" +
      "$3"
  "\\emptylines":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/emptylines"
    prefix: "emptylines"
    body: "\\emptylines" +
      "[${1:number of empty lines}]" +
      "$2"
  "\\externalfigure":
    description: "Include a picture"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/externalfigure"
    prefix: "externalfigure"
    body: "\\externalfigure" +
      "[${1:filename}]" +
      "[${2:options}]" +
      "$3"
  "\\field":
    description: "Form field"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/field"
    prefix: "field"
    body: "\\field" +
      "[${1:name}]" +
      "$2"
  "\\fieldstack":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fieldstack"
    prefix: "fieldstack"
    body: "\\fieldstack" +
      "[${1:name}]" +
      "${2:[${3:names}]}" +
      "${4:[${5:see \\\\setupfields}]}" +
      "$6"
  "\\fillinline":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fillinline"
    description: "A line on which the reader may fill in an answer"
    prefix: "fillinline"
    body: "\\fillinline" +
      "${1:[${2:see \\\\setupfillinlines}]}" +
      "${3:text}" +
      "$4"
  "\\fillinrules":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fillinrules"
    prefix: "fillinrules"
    body: "\\fillinrules" +
      "${1:[${2:see \\\\setupfillinrules}]}" +
      "${{3:text before fill in}}" +
      "${{4:text}}" +
      "$5"
  "\\fillintext":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fillintext"
    prefix: "fillintext"
    body: "\\fillintext" +
      "${1:[${2:see \\\\setupfillinrules}]}" +
      "{${3:text}}" +
      "{${4:text}}" +
      "$5"
  "\\fitfield":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fitfield"
    prefix: "fitfield"
    body: "\\fitfield" +
      "[${1:name}]" +
      "$2"
  "\\fixedspaces":
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fixedspaces"
    description: "Force ~ spacing to be equal to 0 glyph"
    prefix: "fixedspaces"
    body: "\\fixedspaces" +
      "$1"
  "\\followprofile":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/followprofile"
    prefix: "followprofile"
    body: "\\followprofile" +
      "{${1:name}}" +
      "[${2:name}]" +
      "$3"
  "\\followprofileversion":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/followprofileversion"
    prefix: "followprofileversion"
    body: "\\followprofileversion" +
      "{${1:name}}" +
      "[${2:name}]" +
      "[${3:name}]" +
      "$4"
  "\\followversion":
    description: ""
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/followversion"
    prefix: "followversion"
    body: "\\followversion" +
      "{${1:name}}" +
      "[${2:name}]" +
      "$3"
  "\\footnote":
    description: "Set a footnote"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/footnote"
    prefix: "footnote"
    body: "\\footnote" +
      "${1:[${2:label}]}" +
      "{${3:text}}" +
      "$4"
  "\\fraction":
    description: "Typeset a fraction"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/fraction"
    prefix: "fraction"
    body: "\\fraction" +
      "{${1:numerator}}" +
      "{${2:denominator}}" +
      "$3"
  "\\framed":
    description: "Put something in a box"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/framed"
    prefix: "framed"
    body: "\\framed" +
      "${1:[${2: see \\\\setupframed}]}" +
      "{${3:contents}}" +
      "$4"
  "\\from":
    description: "Use an URL"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/from"
    prefix: "from"
    body: "\\from" +
      "[${1:reference}]" +
      "$2"
  "\\getbuffer":
    description: "Place the contents of a previously defined buffer"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/getbuffer"
    prefix: "getbuffer"
    body: "\\getbuffer" +
      "${1:text buffer name}" +
      "$3"
  "\\getmarking":
    description: "Use a section title, page number or the like"
    descriptionMoreURL: "https://wiki.contextgarden.net/Command/getmarking"
